<!DOCTYPE html>
<html  lang="en">
  {{> head }}
  <body >
    
    <div class="error">
      <ul>
      {{!log "errors" errors}}
      {{#each errors as |error|}}
        <li>{{error}}</li>
      {{/each}}
      </ul>
    </div>
    
    <div>
      <ul>
        {{#each sessions as |session|}}
          <li>
            <a href="session/{{@index}}">{{session.name}}</a> @ {{#each this.rooms as |room|}}
            {{room.short}} 
            {{/each}}
            <ul>
                
              {{#each this.groups as |group|}}
                <li> 
                  {{group.name}} : {{!-- x {{group.papers.length}}. --}}
                  {{#each group.papers as |paper|}}
                    {{paper.Subject}} - {{paper.Paper}} : {{paper.Code}}
                  {{/each}}
                </li>
              {{/each}}    
            </ul>

          </li>
        {{/each}}
        <li>
          <a href="/current">Current</a>    
        </li>
        <li> 
          <a href="/boosters">Boosters</a>
        </li>
        <li> 
          <a href="/table">Time Table</a>
        </li>
        <li> 
          <a href="/screen">Time Table for Screens</a>
         </li>
        <li> <a href="/today">Todays Arrangements</a>
          </li>
        
        <li> <a href="/readerpapers">Papers that require Reader PDFs</a>
          </li>
        
          
      </ul>      
      </div>
  </body>
</html>
